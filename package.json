{
  "name": "fuse-box",
  "version": "3.3.0-next.4",
  "description": "Fuse-Box a bundler that does it right",
  "typings": "index.d.ts",
  "main": "index.js",
  "bin": {
    "fuse": "./cli/entry.js"
  },
  "scripts": {
    "help": "node bin/fsbx --help",
    "test": "cross-env FUSE_TEST_TIMEOUT=5000 node ./test.js",
    "test:once": "cross-env gulp dist && TRAVIS=true ./node_modules/mocha/bin/mocha && node ./test.js",
    "start": "gulp watch",
    "devDeps": "gulp installDevDeps",
    "dist": "gulp dist",
    "commit": "git-cz",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "lint": "tslint --project ./src"
  },
  "engines": {
    "node": ">= 8",
    "npm": ">= 3.10.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fuse-box/fuse-box.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fuse-box/fuse-box/issues"
  },
  "homepage": "https://github.com/fuse-box/fuse-box#readme",
  "devDependencies": {
    "@commitlint/cli": "^6.2.0",
    "@commitlint/config-conventional": "^6.1.3",
    "@types/node": "^10.0.9",
    "cheerio": "^1.0.0-rc.2",
    "commitizen": "^2.9.6",
    "cross-env": "^5.1.5",
    "cz-conventional-changelog": "^2.1.0",
    "fuse-test-runner": "^1.0.16",
    "gulp": "3.9.1",
    "gulp-bump": "3.1.1",
    "gulp-clean": "0.3.2",
    "gulp-header": "^2.0.5",
    "gulp-rename": "1.2.2",
    "gulp-replace": "0.6.1",
    "gulp-sourcemaps": "2.6.4",
    "gulp-typescript": "4.0.2",
    "gulp-uglify": "3.0.0",
    "gulp-wrap": "0.13.0",
    "homedir": "^0.6.0",
    "jsdom": "^11.10.0",
    "mocha": "^5.1.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "reflect-metadata": "^0.1.12",
    "run-sequence": "^2.2.1",
    "sinon": "^5.0.7",
    "typescript": "^2.8.3",
    "uglify-js": "^3.3.25",
    "wires-reactive": "^1.1.32"
  },
  "dependencies": {
    "acorn": "^5.5.3",
    "acorn-jsx": "^4.1.1",
    "ansi": "^0.3.1",
    "app-root-path": "^2.0.1",
    "base64-img": "^1.0.4",
    "base64-js": "^1.3.0",
    "chokidar": "^2.0.3",
    "clean-css": "^4.1.11",
    "escodegen": "^1.9.1",
    "express": "^4.16.3",
    "fliplog": "^1.0.4",
    "fs-extra": "^6.0.1",
    "fuse-concat-with-sourcemaps": "^1.0.5",
    "glob": "^7.1.2",
    "ieee754": "^1.1.11",
    "lego-api": "^1.0.8",
    "mustache": "^2.3.0",
    "postcss": "^6.0.22",
    "pretty-time": "^1.0.0",
    "prettysize": "1.1.0",
    "realm-utils": "^1.0.9",
    "request": "^2.85.0",
    "shorthash": "0.0.2",
    "source-map": "^0.7.2",
    "tslib": "^1.9.1",
    "watch": "^1.0.2",
    "ws": "^5.1.1",
    "yargs": "^11.0.0"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/fuse-box",
    "logo": "https://opencollective.com/opencollective/logo.txt"
  },
