    "pretty-time": "^1.1.0",
    "prettysize": "0.0.3",
    "request": "^2.79.0",
    "shorthash": "0.0.2",
    "source-map": "^0.7.3",
    "source-map-support": "^0.5.12",
    "sourcemap-blender": "1.0.5",
    "stream-browserify": "^2.0.1",
    "strict-event-emitter-types": "^2.0.0",
    "terser": "^4.0.0",
    "ts-morph": "^4.1.1",
    "util": "^0.12.0",
